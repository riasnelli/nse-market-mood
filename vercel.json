{
  "version": 2,
  "builds": [
    {
      "src": "api/nse-data.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/nse-data",
      "dest": "/api/nse-data.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}