{
  "version": 2,
  "builds": [
    {
      "src": "api/nse-data.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/dhan-data.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-dhan.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/manifest.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/save-uploaded-data.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/nse-data",
      "dest": "/api/nse-data.js"
    },
    {
      "src": "/api/dhan-data",
      "dest": "/api/dhan-data.js"
    },
    {
      "src": "/api/test-dhan",
      "dest": "/api/test-dhan.js"
    },
            {
              "src": "/manifest.json",
              "dest": "/api/manifest.js",
              "headers": {
                "Content-Type": "application/manifest+json",
                "Access-Control-Allow-Origin": "*"
              }
            },
            {
              "src": "/api/save-uploaded-data",
              "dest": "/api/save-uploaded-data.js"
            },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}